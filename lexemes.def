    "(?<ESCAPE>(\\\\((?&CHARESC)|(?&ASCII)|(?&DECIMAL)|o(?&OCTAL)|x(?&HEXADECIMAL))))"\
    "(?<ESCAPE_NO_AMPERSAND>(\\\\((?&CHARESC_NO_AMPERSAND)|(?&ASCII)|(?&DECIMAL)|o(?&OCTAL)|x(?&HEXADECIMAL))))"\
    "(?<CHAR>('((?&GRAPHIC_NO_BACKSLASH_AND_PRIME)| |(?&ESCAPE_NO_AMPERSAND))'))"\
    "(?<WHITECHAR>(\\n|\\v| |\\t|\\p{Z}))"\
